{
  "name": "cly-lang",
  "displayName": "Cly Language",
  "description": "Syntax highlighting for the Cly language",
  "version": "0.0.1",
  "publisher": "cly-lang",
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cly-lang/cly.h.git"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onLanguage:cly"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cly",
        "aliases": [
          "Cly",
          "cly"
        ],
        "extensions": [
          ".cly"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cly",
        "scopeName": "source.cly",
        "path": "./syntaxes/cly.tmLanguage.json"
      }
    ]
  },
  "author": "Luna Development",
  "license": "MIT"
}
